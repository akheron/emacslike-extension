{
  "scopeName": "source.ag-output",
  "patterns": [
    {
      "match": "^--- .*",
      "name": "comment"
    },
    {
      "match": "^\\(.*",
      "name": "comment"
    },
    {
      "match": "(.*):(\\d+):",
      "captures": {
        "1": { "name": "variable" },
        "2": { "name": "constant.numeric" }
      }
    }
  ]
}
